{
  "name": "clusterio",
  "version": "2.0.0-alpha",
  "description": "Interserver communication for factorio",
  "repository": "https://github.com/clusterio/factorioClusterio",
  "main": "master.js",
  "scripts": {
    "test": "mocha --exit --check-leaks -R spec --recursive test plugins/*/test",
    "fast-test": "FAST_TEST=y npm test",
    "cover": "nyc npm test",
    "fast-cover": "FAST_TEST=y nyc npm test",
    "ci-cover": "nyc --reporter=lcovonly npm run-script test",
    "lint": "eslint \"*.js\" lib plugins test",
    "docs": "jsdoc --configure docs/jsdoc.json",
    "install": "node lib/npmPostinstall",
    "bundle-dependencies": "bundle-dependencies"
  },
  "nyc": {
    "exclude": [
      "test/**",
      "plugins/*/test/**"
    ]
  },
  "keywords": [
    "factorio"
  ],
  "author": "Danielv123",
  "license": "MIT",
  "engines": {
    "node": ">=10.16 <11 || >= 11.13"
  },
  "dependencies": {
    "as-table": "^1.0.31",
    "base64url": "^3.0.0",
    "body-parser": "^1.18.2",
    "chalk": "^3.0.0",
    "cli-interact": "^0.1.9",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "deepmerge": "^4.0.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-fileupload": "1.2.0",
    "follow-redirects": "^1.5.0",
    "fs-extra": "^8.1.0",
    "ini": "^1.3.5",
    "jimp": "^0.12.1",
    "jsonwebtoken": "^8.2.1",
    "jszip": "^3.2.2",
    "lib": "file:./lib",
    "luamin": "^1.0.4",
    "minimist": "^1.2.0",
    "moment": "^2.22.2",
    "needle": "^2.4.0",
    "node-factorio-api": "^0.3.8",
    "node-forge": "^0.10.0",
    "phin": "^3.4.1",
    "pidusage": "^2.0.18",
    "rcon-client": "^4.2.0",
    "request": "^2.83.0",
    "sanitizer": "^0.1.3",
    "semver": "^7.1.1",
    "set-blocking": "^2.0.0",
    "simple-git": "^1.96.0",
    "single-line-log": "^1.1.2",
    "sync-request": "^4.0.1",
    "typescript-collections": "^1.3.2",
    "ws": "^7.2.3",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "html5-validator": "^1.2.1",
    "jsdoc": "^3.6.3",
    "klaw": "^3.0.0",
    "mocha": "^6.2.0",
    "mocha-sinon": "^2.1.0",
    "mocha.parallel": "^0.15.5",
    "nock": "^11.3.0",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1"
  }
}
